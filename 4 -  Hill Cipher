S = AlphabeticStrings()
E = HillCryptosystem (S,4)
R= IntegerModRing (26)
M = MatrixSpace (R,4,4)
A=M([[1,0,1,1], [0,1,1,1],[2,2,3,1], [1,2,0,1]])
print("The Key is: ", A)
Q = S("LAMAISONBLANCHEW")
print("The plain text is", Q)
e = E(A)
C = e(Q)
print("The Cipher Text is", C)
D = E. deciphering (A,C)
print("The Decipher text is", D)
D==Q
